        $.extend(o, {
            Method: requestMerger
        });
        
        function requestMerger() {
            const data = fileDrop.prepareFormData();
            if (data === null)
                return;
            
            const mergeType = $('input[name=mergeType]:checked').val();
            if (fileDrop.droppedFiles.length === 1 && mergeType === "11") {
                const FileAmountMessage = 'Selecione entre dois e dez arquivos Excel';
                showAlert(FileAmountMessage);
                return;
            }
            
            const inputType = getInputType();
        
            const url = o.APIBasePath +
                'MergerApi/Merge?inputType=' + 
                inputType +
                '&outputType=' +
                $('#saveAs').val() +
                '&mergeType=' +
                mergeType +
                "&vertical=" + 
                $('#verticalBlocks').val() +
                "&horizontal=" +
                $('#horizontalBlocks').val();
            request(url, data);
        }
        
        function getInputType() {
            const app = "merger";
            const defaultType = 'xlsx';
            const pathUrl = window.location.pathname.toLowerCase();
            const conversionPos = pathUrl.toLowerCase().indexOf(app);
            if (conversionPos < 0) {
                return defaultType;
            }
            const conv = pathUrl.substring(conversionPos + app.length + 1);
            if (conv.length === 0) {
                return defaultType;
            }
            const arr = conv.split('-');
            return arr[0];
        }
        

        $('#mergeType1').click(function () {
           if ($(this).is(':checked')) {
                $('#verticalBlocks').attr('disabled', true);
                $('#horizontalBlocks').attr('disabled', true);
            }
        });
        $('#mergeType2').click(function () {
            if ($(this).is(':checked')) {
                $('#verticalBlocks').attr('disabled', true);
                $('#horizontalBlocks').attr('disabled', true);
            }
        });
        $('#mergeType3').click(function () {
            if ($(this).is(':checked')) {
                $('#verticalBlocks').attr('disabled', false);
                $('#horizontalBlocks').attr('disabled', false);
            }
        });
    </script>

